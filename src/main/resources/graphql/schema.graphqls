type User {
    id: Int!
    username: String!
    email: String!
    icon: String
    socialAccounts: [SocialAccount]
}

type UserPassword implements User {
    id: Int!
    username: String!
    email: String!
    icon: String
    socialAccounts: [SocialAccount]
    password: String!
    verifiedEmail: Boolean
}

type SocialAccount {
    id: Int!
    provider: String!
    socialAccountId: String!
}

type Query {
    findAllUsers: [User]
}