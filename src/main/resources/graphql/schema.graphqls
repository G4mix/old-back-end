interface User {
    id: Int!
    username: String!
    email: String!
    icon: String
    socialAccounts: [SocialAccount]
    passwordUser: PasswordUser
}

type PasswordUser {
    id: Int!
    user: User!
    password: String!
    verifiedEmail: Boolean
}

type SocialAccount {
    id: Int!
    user: User!
    provider: String!
    socialAccountId: String!
}

type Query {
    findAllUsers: [User]
}